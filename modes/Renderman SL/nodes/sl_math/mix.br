<?xml version="1.0"?>

<node name="Mix" author="Kharitinov Konstantin" icon="mix">
 <help>a*x+b*(1-x)</help>
 <in>
	<param name="a" type="variant" default="0" />
	<param name="b" type="variant" default="0" />
	<param name="x" type="variant" default="0.5" />
 </in>
 <out>
	<param name="result" type="variant">mix(${a},${b},${x})</param>
 </out>
</node>