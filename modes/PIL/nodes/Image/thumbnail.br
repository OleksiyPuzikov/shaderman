<?xml version="1.0"?>
<node name="Image.thumbnail" author="Alexei Puzikov, http://www.dream.com.ua">
 <help>Create thumbnail representation (modifies image in place)</help>
 <in>
  <param name="inp" type="variant" />
  <param name="size" type="variant" default="None" />
  <param name="resample" type="variant" default="0" />
 </in>
 <out>
  <param type="variant" name="outp">${inp}.thumbnail(${size}, ${resample})</param>
</out>
</node>

