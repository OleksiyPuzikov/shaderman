<?xml version="1.0"?>
<node name="Image.getdata" author="Alexei Puzikov, http://www.dream.com.ua">
 <help>Get image data as sequence object.</help>
 <in>
  <param name="inp" type="variant" />
  <param name="band" type="variant" default="None" />
 </in>
 <out>
  <param type="variant" name="outp">${inp}.getdata(${band})</param>
</out>
</node>

