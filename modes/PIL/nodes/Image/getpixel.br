<?xml version="1.0"?>
<node name="Image.getpixel" author="Alexei Puzikov, http://www.dream.com.ua">
 <help>Get pixel value</help>
 <in>
  <param name="inp" type="variant" />
  <param name="xy" type="variant" default="None" />
 </in>
 <out>
  <param type="variant" name="outp">${inp}.getpixel(${xy})</param>
</out>
</node>

