<?xml version="1.0"?>
<node name="Image.offset" author="Alexei Puzikov, http://www.dream.com.ua">
 <help>(deprecated) Offset image in horizontal and/or vertical direction</help>
 <in>
  <param name="inp" type="variant" />
  <param name="xoffset" type="variant" default="None" />
  <param name="yoffset" type="variant" default="None" />
 </in>
 <out>
  <param type="variant" name="outp">${inp}.offset(${xoffset}, ${yoffset})</param>
</out>
</node>

