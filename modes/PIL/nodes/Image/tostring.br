<?xml version="1.0"?>
<node name="Image.tostring" author="Alexei Puzikov, http://www.dream.com.ua">
 <help>Return image as a binary string</help>
 <in>
  <param name="inp" type="variant" />
  <param name="encoder_name" type="variant" default="'raw'" />
 </in>
 <out>
  <param type="variant" name="outp">${inp}.tostring(${encoder_name})</param>
</out>
</node>

