<?xml version="1.0"?>
<node name="Image.save" author="Alexei Puzikov, http://www.dream.com.ua">
 <help>Save image to file or stream</help>
 <in>
  <param name="inp" type="variant" />
  <param name="fp" type="variant" default="None" />
  <param name="format" type="variant" default="None" />
 </in>
 <out>
  <param type="variant" name="outp">${inp}.save(${fp}, ${format})</param>
</out>
</node>

