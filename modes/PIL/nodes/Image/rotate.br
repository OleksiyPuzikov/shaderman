<?xml version="1.0"?>
<node name="Image.rotate" author="Alexei Puzikov, http://www.dream.com.ua">
 <help>Rotate image.  Angle given as degrees counter-clockwise.</help>
 <in>
  <param name="inp" type="variant" />
  <param name="angle" type="variant" default="None" />
  <param name="resample" type="variant" default="0" />
 </in>
 <out>
  <param type="variant" name="outp">${inp}.rotate(${angle}, ${resample})</param>
</out>
</node>

