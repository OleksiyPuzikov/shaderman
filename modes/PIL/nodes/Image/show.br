<?xml version="1.0"?>
<node name="Image.show" author="Alexei Puzikov, http://www.dream.com.ua">
 <help>Display image (for debug purposes only)</help>
 <in>
  <param name="inp" type="variant" />
  <param name="title" type="variant" default="None" />
  <param name="command" type="variant" default="None" />
 </in>
 <out>
  <param type="variant" name="outp">${inp}.show(${title}, ${command})</param>
</out>
</node>

