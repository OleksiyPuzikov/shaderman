<?xml version="1.0"?>
<node name="Image.histogram" author="Alexei Puzikov, http://www.dream.com.ua">
 <help>Take histogram of image</help>
 <in>
  <param name="inp" type="variant" />
  <param name="mask" type="variant" default="None" />
  <param name="extrema" type="variant" default="None" />
 </in>
 <out>
  <param type="variant" name="outp">${inp}.histogram(${mask}, ${extrema})</param>
</out>
</node>

