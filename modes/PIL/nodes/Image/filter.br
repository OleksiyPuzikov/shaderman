<?xml version="1.0"?>
<node name="Image.filter" author="Alexei Puzikov, http://www.dream.com.ua">
 <help>Apply environment filter to image</help>
 <in>
  <param name="inp" type="variant" />
  <param name="filter" type="variant" default="None" />
 </in>
 <out>
  <param type="variant" name="outp">${inp}.filter(${filter})</param>
</out>
</node>

