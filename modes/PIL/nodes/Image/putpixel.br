<?xml version="1.0"?>
<node name="Image.putpixel" author="Alexei Puzikov, http://www.dream.com.ua">
 <help>Set pixel value</help>
 <in>
  <param name="inp" type="variant" />
  <param name="xy" type="variant" default="None" />
  <param name="value" type="variant" default="None" />
 </in>
 <out>
  <param type="variant" name="outp">${inp}.putpixel(${xy}, ${value})</param>
</out>
</node>

