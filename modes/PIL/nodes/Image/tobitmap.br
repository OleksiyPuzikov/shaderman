<?xml version="1.0"?>
<node name="Image.tobitmap" author="Alexei Puzikov, http://www.dream.com.ua">
 <help>Return image as an XBM bitmap</help>
 <in>
  <param name="inp" type="variant" />
  <param name="name" type="variant" default="'image'" />
 </in>
 <out>
  <param type="variant" name="outp">${inp}.tobitmap(${name})</param>
</out>
</node>

