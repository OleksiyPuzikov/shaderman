<?xml version="1.0"?>
<node name="Image.crop" author="Alexei Puzikov, http://www.dream.com.ua">
 <help>Crop region from image</help>
 <in>
  <param name="inp" type="variant" />
  <param name="box" type="variant" default="None" />
 </in>
 <out>
  <param type="variant" name="outp">${inp}.crop(${box})</param>
</out>
</node>

